<tr>
  <td>
    <%= link_to(match) do %>Match <%= match.id %><% end %>
  </td>
  <td>
    <div class= "tag <%= match.player_description(match.player_x) %>"><%= match.player_x.name %></div>
  </td>
  <td>
    <div class= "tag <%= match.player_description(match.player_o) %>"><%= match.player_o.name %></div>
  </td>
  <td>
    <%= match.description %>
  </td>
</tr>